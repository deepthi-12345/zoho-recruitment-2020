#include<stdio.h>

int main()
{
	
    int i,j,N;
    printf("Input:");
    printf("\n");
    scanf("%d", &N);

    for(i=N; i>=1; i--)
    {
        for(j=N; j>i; j--)
        {
             printf("%d",j);       
        }
        for(j=1; j<=i; j++)
        {
            printf("%d", i);
        }
        printf("\n");
    }
}
